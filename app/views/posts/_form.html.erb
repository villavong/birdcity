<%= form_for ([@student, @post]) do |f| %>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  <div class="field">
    <%= f.label "Upload" %> PHOTO!!<br>
    <%= f.file_field :photo %>
  </div>
  <br><br>
  <div class="field">
    <%= f.label "Upload" %> MP3 !!!<br>
    <%= f.file_field :mp3 %>
  </div>
  <br><br>
  <%= f.submit "Create Post", class: "button_pink button_highlight button_block" %>
<% end %>
